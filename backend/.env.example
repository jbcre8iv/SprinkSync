# SprinkSync Backend Environment Configuration
# Copy this file to .env and adjust values as needed

# Environment
NODE_ENV=development

# Server
PORT=3000

# Database
DATABASE_PATH=./database/sprinkSync.db

# GPIO Mode (mock for development, real for Raspberry Pi)
GPIO_MODE=mock

# Logging
LOG_LEVEL=debug

# Application
APP_NAME=SprinkSync
